# Copy this file to `.env` and fill values locally (DO NOT COMMIT .env)
# VAPI (voice assistant) credentials
VAPI_API_KEY=
VAPI_BASE_URL=https://api.vapi.ai
VAPI_ASSISTANT_ID=

# GROQ (reasoning/LLM) credentials
GROQ_API_KEY=
# Optional model override
GROQ_MODEL=llama-3.1-8b-instant

# SMTP (email) settings for alerts
SMTP_HOST=
SMTP_PORT=587
SMTP_USER=
SMTP_PASS=
EMAIL_FROM=
HOSPITAL_ALERT_TO=your-alert-email@example.com

# Supabase (frontend) - expose only anon/public keys using NEXT_PUBLIC_
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
NEXT_PUBLIC_API_URL=/api
